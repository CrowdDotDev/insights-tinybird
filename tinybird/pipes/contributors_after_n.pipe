TOKEN "read" READ

NODE endpoint_contributors_after_n
SQL >

    %
        SELECT 
            ACTOR_LOGIN,
            ACTOR_AVATAR_URL,
            SUM(commit_count) as commit_count
        FROM commit_aggs
        GROUP BY ACTOR_LOGIN, ACTOR_AVATAR_URL
        ORDER BY commit_count DESC
        LIMIT {{ Int32(limit) }}
        OFFSET {{ Int32(offset) }} 